<Window x:Class="C3.Views.ReportManuallyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:C3.Views"
        mc:Ignorable="d"
        Title="ReportManuallyWindow">
    <StackPanel>
        <TextBlock x:Name="TextEnterUrl" Width="640" Height="20" FontSize="15" TextAlignment="Center">
            <Run x:Name="TextEnterUrl_Text" Text="Enter a URL to to report."/>
        </TextBlock>
        <TextBox x:Name="TextBox_ReportManually" Width="640" Height="40.87" HorizontalAlignment="Center"  Background="Transparent" BorderThickness="2" VerticalContentAlignment="Center" Foreground="Black" FontFamily="../Resources/#AppleSDGothicNeoB00" FontSize="16"/>
        <Button x:Name="BtnReportManually" Width="640" Height="40" Click="BtnReportManually_Click">
            <TextBlock x:Name="BtnReportManually_Text" Text="Report"/>
        </Button>
    </StackPanel>
</Window>
